{
  "apiVersion": "1",
  "name": "osm/open-street-map",
  "title": "Open Street Map",
  "description": "Enhance your WordPress site with interactive open-street-map (OSM) blocks.",
  "category": "widgets",
  "keywords": [
    "OSM",
    "osm",
    "Open Street Map",
    "open",
    "Open",
    "Map",
    "map",
    "open street map"
  ],
  "textdomain": "open-street-map",
  "attributes": {
    "cId": {
      "type": "string",
      "default": ""
    },
    "align": {
      "type": "string",
      "default": "center"
    },
    "map": {
      "type": "object",
      "default": {
        "marker": {
          "url": "https://png.pngtree.com/png-clipart/20230123/original/pngtree-flat-red-location-sign-png-image_8927579.png",
          "toUrl": "https://png.pngtree.com/png-clipart/20230123/original/pngtree-flat-red-location-sign-png-image_8927579.png",
          "pathUrl": "https://i.pinimg.com/474x/94/df/b6/94dfb68e3b5ef24ec89c23044247ea91.jpg"
        },
        "listPlace": [],
        "selectPosition": "",
        "searchQuery": "",
        "latitude": "",
        "longitude": "",
        "routeDirection": {
            "myLocation": {
              "lat": 0,
              "lon": 0,
              "locationName": ""
            },
            "toLocation": {
              "lat": 0,
              "lon": 0,
              "locationName": ""
            }
        }
      }
    },
    "options": {
      "type": "object",
      "default": {
        "scrollZoom": true,
        "isMapLayer": true,
        "isFullScreen": true,
        "isViewOtherAddress": true,
        "isViewMyLocation": true,
        "mapLayerType": "default",
        "selectMarker": "start"
      }
    },
    "layout": {
      "type": "object",
      "default": {
        "mapColumns": {
          "width": { "desktop": "620px", "tablet": "620px", "mobile": "300px" },
          "height": { "desktop": "450px", "tablet": "450px", "mobile": "295px" }
        },
        "selfMarkerColumns": {
          "width": { "desktop": 40, "tablet": 35, "mobile": 30 },
          "height": { "desktop": 40, "tablet": 35, "mobile": 30 }
        },
        "othersMarkerColumns": {
          "width": { "desktop": 40, "tablet": 35, "mobile": 30 },
          "height": { "desktop": 40, "tablet": 35, "mobile": 30 }
        },
        "pathMarkerColumns": {
          "width": { "desktop": 40, "tablet": 35, "mobile": 30 },
          "height": { "desktop": 40, "tablet": 35, "mobile": 30 }
        }
      }
    },
    "style": {
      "type": "object",
      "default": {
        "tooltipColors": { "color": "white", "bg": "#4527a4" },
        "tooltipTypo": {},
        "border": {},
        "padding": {
          "desktop": {},
          "tablet": {},
          "mobile": {}
        },
        "zoom": 15
      }
    }
   
  },
  "supports": {
    "align": ["wide", "full"],
    "html": false
  },
  "example": {
    "attributes": {
      "preview": true,
      "columns": {
        "desktop": 1,
        "tablet": 1,
        "mobile": 1
      }
    }
  },
  "editorScript": "file:./../dist/editor.js"
}
